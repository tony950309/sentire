<?xml version="1.0" encoding="utf-8"?>
<android.support.constraint.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context="com.example.jiachenggu.sentirepro.seeq">

    <ImageButton
        android:id="@+id/imbtnseeqpre"
        android:layout_width="180dp"
        android:layout_height="50dp"
        android:layout_marginBottom="8dp"
        android:layout_marginLeft="8dp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintLeft_toLeftOf="parent"
        app:srcCompat="@android:drawable/ic_media_previous"
        android:layout_marginStart="8dp" />

    <ImageButton
        android:id="@+id/imbtnseeqnext"
        android:layout_width="180dp"
        android:layout_height="50dp"
        android:layout_marginBottom="9dp"
        android:layout_marginRight="9dp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintRight_toRightOf="parent"
        app:srcCompat="@android:drawable/ic_media_next"
        android:layout_marginEnd="9dp" />

    <TextView
        android:id="@+id/tvseeqnumb"
        android:layout_width="187dp"
        android:layout_height="42dp"
        android:layout_marginBottom="9dp"
        android:layout_marginEnd="8dp"
        android:layout_marginLeft="8dp"
        android:layout_marginRight="8dp"
        android:layout_marginStart="8dp"
        android:gravity="center"
        android:textSize="18sp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintLeft_toRightOf="@+id/imbtnseeqpre"
        app:layout_constraintRight_toLeftOf="@+id/imbtnseeqnext" />

    <TextView
        android:id="@+id/textView11"
        android:layout_width="614dp"
        android:layout_height="52dp"
        android:layout_marginBottom="160dp"
        android:layout_marginLeft="8dp"
        android:layout_marginRight="8dp"
        android:background="#00b1ae"
        android:text="Private Information"
        android:textAlignment="center"
        android:textColor="#000000"
        android:textSize="24sp"
        app:layout_constraintBottom_toTopOf="@+id/tvseeqnumb"
        app:layout_constraintHorizontal_bias="0.517"
        app:layout_constraintLeft_toLeftOf="parent"
        app:layout_constraintRight_toRightOf="parent"
        android:layout_marginStart="8dp"
        android:layout_marginEnd="8dp" />

    <TextView
        android:id="@+id/tvseeqNAG"
        android:layout_width="183dp"
        android:layout_height="132dp"
        android:layout_marginBottom="13dp"
        android:layout_marginLeft="16dp"
        android:layout_marginStart="16dp"
        android:textSize="18sp"
        app:layout_constraintBottom_toTopOf="@+id/imbtnseeqpre"
        app:layout_constraintLeft_toLeftOf="parent" />

    <TextView
        android:id="@+id/tvseeqdisab"
        android:layout_width="355dp"
        android:layout_height="130dp"
        android:layout_marginBottom="12dp"
        android:layout_marginEnd="16dp"
        android:layout_marginRight="16dp"
        android:textSize="14sp"
        app:layout_constraintBottom_toTopOf="@+id/imbtnseeqpre"
        app:layout_constraintRight_toRightOf="parent" />

    <ListView
        android:id="@+id/lvseeq"
        android:layout_width="600dp"
        android:layout_height="558dp"
        android:layout_marginBottom="8dp"
        android:layout_marginLeft="0dp"
        android:layout_marginRight="8dp"
        android:layout_marginTop="2dp"
        app:layout_constraintBottom_toTopOf="@+id/textView11"
        app:layout_constraintHorizontal_bias="0.571"
        app:layout_constraintLeft_toLeftOf="parent"
        app:layout_constraintRight_toRightOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.538" />
</android.support.constraint.ConstraintLayout>
